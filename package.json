{
  "name": "@discord-interactions/discord-interactions",
  "version": "0.0.1",
  "scripts": {
    "clean": "lerna clean",
    "build:lerna": "lerna run build",
    "build:request": "bin/pb request",
    "build:api": "bin/pb api",
    "build:builders": "cd packages/builders && npm install && npm run build && npm link && cd ../",
    "build:core": "cd packages/core && npm install && npm run build && npm link && cd ../",
    "build:ifp": "bin/pb isomorphic-fetch-ponyfill",
    "build:verify": "bin/pb verify",
    "build": "npm run build:ifp && npm run build:request && npm run build:api && npm run build:builders && npm run build:core && npm run build:verify",
    "format": "lerna run format",
    "lint": "lerna run lint",
    "docs": "lerna run docs",
    "test": "lerna run test",
    "example": "yarn build && lerna run dev --scope=@discord-interactions/example-bot",
    "example:worker": "yarn build && lerna run dev --scope=@discord-interactions/example-bot-worker",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/node": "^18.19.50",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.19.0",
    "jest": "^28.1.2",
    "lerna": "^8.1.8",
    "nx": "^19.7.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "typedoc": "^0.26.7",
    "typedoc-plugin-markdown": "^4.2.7",
    "typescript": "^4.9.5",
    "yarn": "^1.22.22"
  },
  "private": true,
  "engines": {
    "node": ">=18.20.4 <19"
  },
  "packageManager": "yarn@4.4.1"
}
